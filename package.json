    "clean": "rm -rf lib_es6 lib docs_src/webpack/*.js dist docs_ts_compiled docs",
    "build_es6": "tsc --project tsconfig.lib.json",
    "es6_to_es5": "babel lib_es6 --out-dir lib",
    "build_lib": "npm run build_es6 && npm run es6_to_es5 && npm run generate_dts",
    "prepublish": "npm run clean && npm run build",
    "generate_dts": "node dts-generator.js",
    "build": "npm run build_lib && tsc && npm run build-storybook",
    "storybook": "tsc --project tsconfig.json && start-storybook -p 7001",
    "build-storybook": "build-storybook",
    "test": "npm run unittest",
    "unittest": "jest --config src/__unittests__/jest_unittests.json"
