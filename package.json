{
  "scripts": {
    "clean": "rm -rf lib_es6 lib docs_src/webpack/*.js dist docs_ts_compiled docs reactive-renderer.d.ts",
    "build_es6": "tsc --project tsconfig.lib.json",
    "es6_to_es5": "babel lib_es6 --out-dir lib",
    "build_lib": "npm run build_es6 && npm run es6_to_es5 && npm run generate_dts",
    "prepublish": "npm run clean && npm run build",
    "generate_dts": "ts-node tools/dts-generator.ts",
    "build": "npm run build_lib && tsc",
    "storybook": "tsc --project tsconfig.json && start-storybook -p 7001",
    "test": "jest",
    "aatest": "npm run unittest",
    "aaunittest": "jest --config src/__unittests__/jest_unittests.json"
  },
  "name": "reactive-renderer",
  "version": "0.0.1",
  "description": "Reactive Renderer",
  "main": "lib/index.js",
  "types": "reactive-renderer.d.ts",
  "repository": "https://github.com/react-stream-libs/reactive-renderer.git",
  "author": "devdoomari <devdoomari __at__ gmail dotttt commmm>",
  "license": "Personal",
  "devDependencies": {
    "@babel/cli": "^7.8.0",
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-async-generator-functions": "^7.2.0",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/react": "^5.1.10",
    "@types/chai": "^4.1.7",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/dts-generator": "^2.1.4",
    "@types/jest": "^24.0.16",
    "@types/lodash": "^4.14.136",
    "@types/ramda": "^0.26.18",
    "@types/react": "^16.8.24",
    "@types/storybook__react": "^4.0.2",
    "@types/table": "^4.0.6",
    "@types/uuid": "^3.4.5",
    "@types/webpack": "^4.32.1",
    "chai": "^4.2.0",
    "dts-generator": "^3.0.0",
    "jest": "^24.8.0",
    "lodash": "^4.17.15",
    "monaco-editor": "^0.17.1",
    "parcel-bundler": "^1.12.3",
    "table": "^5.4.5",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
